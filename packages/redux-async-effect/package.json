{
  "name": "redux-async-effect",
  "version": "0.1.0",
  "description": "Consume a stream of and emit Redux actions with an async function/method or generator",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "cd src && tsc && tsc -m es6 --outDir ../lib-esm",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare" : "npm run build",
    "prepublishOnly" : "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mascarenhas/async-effects.git"
  },
  "keywords": [
    "ngrx",
    "async",
    "effects",
    "sagas",
    "generators"
  ],
  "author": "Fabio Mascarenhas <fabio.mascarenhas@rangle.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mascarenhas/async-effects/issues"
  },
  "homepage": "https://github.com/mascarenhas/async-effects#readme",
  "peerDependencies": {
    "rxjs": "^6.0.0",
    "typescript": ">=3.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "jest": "^24.7.1",
    "prettier": "^1.16.4",
    "rxjs": "^6.4.0",
    "ts-jest": "^24.0.1",
    "typescript": "~3.2.4"
  },
  "files": [
    "lib",
    "lib-esm"
  ],
  "jest": {
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testRegex": "(\\.|/)spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }
}
